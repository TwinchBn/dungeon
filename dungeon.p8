function _init()
	log={}
	--make_map()
	gravity=.2
	make_player()
end

function _update()
	update_player()
end

function _draw()
	cls()
	camera(0,0)
	map(0,0)
	camera()
	draw_player(true)
	--debug()
end

function debug()
	local gap = 3
	local x,y,w,h = 0,0,0,0
	for i=1,#log do
		local w2 = #(log[i])*4 + gap*2
		if w2  > w then
			w=w2
		end
	end
	h = #log*(5+gap)+gap
	x,y = 127-w,127-h
	rectfill(x,y,x+w,y+h,1)
	rect(x,y,x+w,y+h,8)
	for i = 1,#log do
		print(log[i],x+gap,y+gap+(i-1)*(5+gap),6)
	end
end
