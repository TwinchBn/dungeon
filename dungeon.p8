function lerp(a,b,t)
	return a + t * (b-a)
end --lerp()

function movet(a,b,m)
	local d = abs(b-a) --distance
	if (d<=m) return b
	--if (b<a) m *= -1
	--return a + d * m
	return a + sgn(b-a) * m
end --movetowards



